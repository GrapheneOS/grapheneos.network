#!/bin/bash

set -o errexit -o nounset -o pipefail

sha256() {
    echo -n "$(basename $1): "
    curl -s "$1" | sha256sum | cut -d ' ' -f 1
}

echo Broadcom

echo

echo gllto.glpals.com
sha256 https://gllto.glpals.com/7day/v5/latest/lto2.dat
sha256 https://gllto.glpals.com/rtistatus4.dat
sha256 https://gllto.glpals.com/rto/v1/latest/rto.dat

echo

echo agnss.goog
sha256 https://agnss.goog/lto2.dat
sha256 https://agnss.goog/rtistatus.dat
sha256 https://agnss.goog/rto.dat

echo

echo broadcom.psds.grapheneos.org
sha256 https://broadcom.psds.grapheneos.org/lto2.dat
sha256 https://broadcom.psds.grapheneos.org/rtistatus.dat
sha256 https://broadcom.psds.grapheneos.org/rto.dat

echo

echo Qualcomm

echo

echo path1.xtracloud.net
sha256 https://path1.xtracloud.net/xtra3Mgrbeji.bin
sha256 https://path1.xtracloud.net/xtra3grcej.bin

echo

echo path2.xtracloud.net
sha256 https://path2.xtracloud.net/xtra3Mgrbeji.bin
sha256 https://path2.xtracloud.net/xtra3grcej.bin

echo

echo path3.xtracloud.net
sha256 https://path3.xtracloud.net/xtra3Mgrbeji.bin
sha256 https://path3.xtracloud.net/xtra3grcej.bin

echo

echo qualcomm.psds.grapheneos.org
sha256 https://qualcomm.psds.grapheneos.org/xtra3Mgrbeji.bin
sha256 https://qualcomm.psds.grapheneos.org/xtra3grcej.bin
